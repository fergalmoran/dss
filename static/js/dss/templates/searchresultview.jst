<li>
    <a href="<%= item_url %>">
        <img src="<%= mix_image %>" class="msg-photo" alt="<%= title %>">
    <span class="msg-body">
        <span class="msg-title">
            <span class="blue"><%= user_name %>:</span>
            <%= title %>
        </span>

        <span class="msg-time">
            <i class="icon-time"></i>
            <span><%= moment(upload_date).fromNow() %></span>
        </span>
    </span>
    </a>
</li>